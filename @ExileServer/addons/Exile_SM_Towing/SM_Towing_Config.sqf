/*
	Allow players to tow in safezone?
*/
SMCanTowInSafeZone = true;

/*
	Turning this on will ignore SMCanBeTowedVehicles and SMCanTowVehicles and allow you tow anything .
	This could be problematic for large vehicles although it has been tested with the Ikarus
	If the vehicle does not have a class in the config.cpp it will use the default value.
*/
SMCanTowEverything = false;

/*
	Players need an item to be able to tow?
*/
SMNeedsItemToTow = true;

/*
	If so what item? (supports mutliple items!)
	If you add more items don't forget to add , !
*/
SMItemsNeededToTow =
[
	"Exile_Item_Rope"
];

/*
	Will require 2 "Rope" and 2 "MetalScrews"
	Example:
	SMItemsNeededToTow =
	[
		//2 Ropes
		"Exile_Item_Rope",
		"Exile_Item_Rope",
		//2 nails
		"Exile_Item_MetalScrews",
		"Exile_Item_MetalScrews"
		//Make sure there isint a , at the end!
	];
*/

